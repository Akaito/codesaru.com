<?xml version="1.0" ?>
<ScoreSet version="1" style="{{ style }}">
    <String name="level_id">{{ level_id }}</String>
    <Integer name="category" value="5"/>
{% for score in scores %}
    <Score>
        <String name="initials">{{ score.initials }}</String>
        <Float name="time" value="{{ score.time }}"/>
        <Integer name="score" value="{{ score.score }}"/>
        <Integer name="biggest_trick" value="{{ score.biggest_trick }}"/>
        <String name="date">{{ score.date }}</String>
        <Integer name="score_id" value="{{ score.id }}"/>
    </Score>
{% endfor %}
{% if desc %}
    <String name="desc">{{ desc }}</String>
{% endif %}
</ScoreSet>

